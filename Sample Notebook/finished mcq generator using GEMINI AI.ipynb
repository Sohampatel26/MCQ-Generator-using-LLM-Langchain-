      "cell_type": "code",
      "execution_count": 24,
      "metadata": {
        "id": "G42Zg_9OxcII"
      },
      "outputs": [],
      "source": [
        "review_chain = LLMChain(llm=llm, prompt=quiz_evaluation_prompt, output_key=\"review\", verbose=True)"
      ]
    },
