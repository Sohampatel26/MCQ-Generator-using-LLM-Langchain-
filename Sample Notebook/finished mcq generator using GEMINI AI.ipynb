      "outputs": [],
      "source": [
        "# Sequential Chain\n",
        "generate_evaluate_chain = SequentialChain(\n",
        "    chains=[quiz_chain, review_chain],\n",
        "    input_variables=[\"text\", \"number\", \"subject\", \"tone\", \"response_json\"],\n",
        "    output_variables=[\"quiz\", \"review\"],\n",
        "    verbose=True\n",
        ")"
      ]
